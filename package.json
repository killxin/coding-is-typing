{
	"name": "coding-is-typing",
	"displayName": "Coding is typing",
	"description": "learn something by typing it",
	"version": "1.0.0",
	"publisher": "rhjiang",
	"repository": "https://github.com/killxin/coding-is-typing",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"icon": "media/icon.png",
	"activationEvents": [
		"onCommand:codeTyping.start"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"title": "Enable CodeTyping",
				"command": "codeTyping.start",
				"category": "Coding is typing"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"monaco-editor": "^0.18.1",
		"typewriter-noises": "git+https://github.com/timmyreilly/TypewriterNoises-VSCode.git"
	}
}
